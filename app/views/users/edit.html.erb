<h1>Edit your profile</h1>

<%= form_with model: @user, url: user_path(@user), method: :patch do |form| %>
  <div>
    <%= form.label :username, "Current Username: #{@user.username}" %>
    <%= form.text_field :username, id: :username, required: true %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>